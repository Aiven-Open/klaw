{
  "name": "coral",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "",
  "license": "Apache-2.0",
  "engineStrict": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=8.0.0 <9.0.0",
    "yarn": "❗Please use pnpm to assure a consistent package management.",
    "npm": "❗Please use pnpm to assure a consistent package management."
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --mode remote-api",
    "start": "pnpm dev",
    "dev-without-api": "vite",
    "lint": "prettier --check . && eslint .",
    "lint-staged": "lint-staged",
    "preview": "vite build --mode remote-api && vite preview --mode remote-api",
    "reformat": "prettier --write . proxy",
    "test": "jest",
    "test-ci": "jest --ci",
    "test-dev": "jest --watch",
    "tsc": "tsc -p tsconfig.json",
    "bundle-analyze": "BUNDLE_ANALYZE=1 vite build",
    "extract-api-types": "openapi-typescript ../openapi.yaml --output types/api.d.ts --make-paths-enum --export-type",
    "add-precommit": "git config --local core.hooksPath .githooks/",
    "generate-toc": "node scripts/markdown-toc.cjs && prettier --write '**/*.md' '../**/*.md'"
  },
  "lint-staged": {
    "**/!(*api.d).{ts,tsx,js,cjs}": [
      "prettier --check",
      "eslint"
    ],
    "**/*.{md, css}": [
      "prettier --check"
    ]
  },
  "dependencies": {
    "@aivenio/aquarium": "1.57.1",
    "@hookform/resolvers": "^2.9.10",
    "@monaco-editor/react": "^4.6.0",
    "@tanstack/react-query": "^4.29.5",
    "@types/react-syntax-highlighter": "^15.5.11",
    "monaco-editor": "^0.46.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.22.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-parse": "^9.0.0",
    "rehype-remark": "^10.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.29.6",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-react": "^7.33.2",
    "glob": "^10.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.1",
    "lodash": "^4.17.21",
    "msw": "^2.2.1",
    "openapi-typescript": "^6.7.4",
    "prettier": "^3.2.4",
    "prop-types": "^15.8.1",
    "react-aria": "^3.32.1",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vite-plugin-svgr": "^4.2.0",
    "whatwg-fetch": "^3.6.20"
  },
  "peerDependencies": {
    "json5": ">=1.0.2",
    "yaml": "2.2.2"
  },
  "alex": {
    "noBinary": true,
    "profanitySureness": 1
  }
}
